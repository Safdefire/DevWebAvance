package fr.imt.lgi2p.pcmd;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

@Entity
public class Candidature {

	
	public Mobilite choixMobilite;
	public Etudiant etudiant;

	public Candidature(Mobilite choixMobilite, Etudiant etudiant) {
		super();
		this.choixMobilite = choixMobilite;
		this.etudiant = etudiant;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((choixMobilite == null) ? 0 : choixMobilite.hashCode());
		result = prime * result + ((etudiant == null) ? 0 : etudiant.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Candidature other = (Candidature) obj;
		if (choixMobilite == null) {
			if (other.choixMobilite != null)
				return false;
		} else if (!choixMobilite.equals(other.choixMobilite))
			return false;
		if (etudiant == null) {
			if (other.etudiant != null)
				return false;
		} else if (!etudiant.equals(other.etudiant))
			return false;
		return true;
	}

	public Mobilite getChoixMobilite() {
		return choixMobilite;
	}

	public void setChoixMobilite(Mobilite choixMobilite) {
		this.choixMobilite = choixMobilite;
	}

	public Etudiant getEtudiant() {
		return etudiant;
	}

	public void setEtudiant(Etudiant etudiant) {
		this.etudiant = etudiant;
	}
}
