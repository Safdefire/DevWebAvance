    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "attach",
            "name": "Chromium Launch",
            "url": "http://127.0.0.1:4200/",            
            "port":9222,
            "sourceMaps": true,
            "webRoot": "${workspaceFolder}",
            "sourceMapPathOverrides": {
                "webpack:///./*":"${workspaceRoot}\\*",
            }
        }
    ]
}